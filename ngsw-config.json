{
  	"$schema": "./node_modules/@angular/service-worker/config/schema.json",
  	"index": "/index.html",
  	"assetGroups": [
		{
			"name": "app",
			"installMode": "prefetch",
			"resources": {
				"files": [
					"/favicon.ico",
					"/index.html",
					"/*.css",
					"/*.js"
				]
			}
		}, 
		{
			"name": "assets",
			"installMode": "lazy",
			"updateMode": "prefetch",
			"resources": {
				"files": [
					"/assets/**",
					"/*.(eot|svg|cur|jpg|png|webp|gif|otf|ttf|woff|woff2|ani)"
				]
			}
		}
	],
	"dataGroups": [
		{
			"name": "mockapi.io",
			"urls": ["https://5a5a9e00bc6e340012a03796.mockapi.io/**"],
			"cacheConfig": {
				"maxSize": 3,
				"maxAge": "7d",
				"strategy": "freshness"
			}
		}
	]
	  
}
